{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test": "TEST_MODE=on vitest",
    "build": "tsup src/index.ts --format cjs --publicDir",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsup src/index.ts --format cjs --publicDir --watch --onSuccess \"node dist/index.js\"",
    "lint": "tsc --noEmit && eslint \"src/**/*.ts*\"",
    "start": "node dist/index.js"
  },
  "main": "dist/index.js",
  "engines": {
    "node": "18.x"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-openapi-validator": "^5.0.4",
    "morgan": "^1.10.0",
    "pino": "^8.14.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.12.2",
    "@types/supertest": "^2.0.12",
    "eslint": "^7.32.0",
    "pino-pretty": "^10.0.1",
    "supertest": "^6.2.4",
    "tsup": "^7.1.0",
    "vite": "^4.4.2",
    "typescript": "^4.8.3",
    "vite-plugin-node": "^3.0.2",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.33.0"
  }
}
