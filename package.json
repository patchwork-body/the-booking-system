{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "format": "prettier --write \"src/**/*.ts*\"",
    "test:e2e": "TEST_MODE=on vitest --threads false",
    "build": "tsup src/index.ts --format cjs --publicDir",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsup src/index.ts --format cjs --publicDir --watch --onSuccess \"node dist/index.js | pino-pretty\"",
    "lint": "tsc --noEmit && eslint \"src/**/*.ts*\"",
    "start": "node dist/index.js",
    "postinstall": "prisma generate"
  },
  "main": "dist/index.js",
  "engines": {
    "node": "18.x"
  },
  "dependencies": {
    "@prisma/client": "5.0.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-openapi-validator": "^5.0.4",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.1",
    "morgan": "^1.10.0",
    "pino": "^8.14.1",
    "tiny-invariant": "^1.3.1",
    "zod": "^3.21.4",
    "zod-express-middleware": "^1.4.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.12.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.8.0",
    "pino-pretty": "^10.0.1",
    "prettier": "^3.0.0",
    "prisma": "^5.0.0",
    "supabase": "^1.77.9",
    "supertest": "^6.2.4",
    "tsup": "^7.1.0",
    "typescript": "^4.8.3",
    "vite": "^4.4.2",
    "vite-plugin-node": "^3.0.2",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.33.0"
  }
}
